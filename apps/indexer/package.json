{
  "name": "@swr/indexer",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ponder dev --schema swr_dev",
    "start": "ponder start --schema swr_prod",
    "db": "ponder db",
    "db:wipe": "dotenv -e .env.local -- bash -c \"psql $DATABASE_URL -c 'DROP SCHEMA IF EXISTS swr_dev CASCADE; DROP SCHEMA IF EXISTS swr_prod CASCADE; DROP SCHEMA IF EXISTS public CASCADE; CREATE SCHEMA public;'\"",
    "codegen": "ponder codegen",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@swr/abis": "workspace:*",
    "@swr/chains": "workspace:*",
    "hono": "^4.5.0",
    "ponder": "^0.16.1",
    "viem": "^2.41.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
