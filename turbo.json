{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**"]
    },
    "landing#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "forge:build": {
      "dependsOn": [],
      "outputs": ["out/**"],
      "inputs": ["src/**", "test/**", "script/**", "foundry.toml"]
    },
    "forge:test": {
      "dependsOn": ["forge:build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "format": {
      "cache": false
    },
    "format:check": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "build-storybook": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**"]
    },
    "@swr/docs#dev": {
      "cache": false,
      "persistent": true
    },
    "@swr/docs#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@swr/docs#lint": {
      "dependsOn": ["^build"]
    },
    "@swr/docs#typecheck": {
      "dependsOn": ["^build"]
    },
    "@swr/docs#format": {
      "cache": false
    },
    "@swr/docs#format:check": {
      "outputs": []
    }
  }
}
