{
  "name": "stolen-wallet-registry-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "check": "turbo lint typecheck test && pnpm format:check",
    "clean": "turbo clean && rm -rf node_modules",
    "prepare": "husky",
    "storybook": "turbo storybook",
    "build-storybook": "turbo build-storybook",
    "anvil": "anvil --ipc /tmp/anvil.ipc --steps-tracing --block-time 13"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "turbo": "^2.6.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=9.0.0"
  }
}
